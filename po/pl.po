#: ogg123/cmdline_options.c:331
#, c-format
msgid ""
"  -d dev, --device dev    Use output device \"dev\". Available devices:\n"
msgstr ""
"  -d urz, --device urz    Użycie urządzenia wyjściowego \"urz\". Dostępne:\n"

