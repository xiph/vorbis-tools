msgstr "UWAGA: nierozpoznane nominalne bitrate \"%s\"\n"

#: oggenc/oggenc.c:828
#, c-format
msgid "WARNING: minimum bitrate \"%s\" not recognised\n"
msgstr "UWAGA: nierozpoznane minimalne bitrate \"%s\"\n"

#: oggenc/oggenc.c:841
